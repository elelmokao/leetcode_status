      "targets": [
        {
          "dataset": "postgres",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${DS_POSTGRES}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH daily_unique_stats AS (\n    -- 第一步：確保每一天 (day) 的每一題 (problem_id) 只有一筆資料\n    -- 我們取每一天中 collected_at 最晚的那一筆\n    SELECT \n        date_trunc('day', a.collected_at) AS \"time\",\n        a.problem_id,\n        (a.total_accepted::float / NULLIF(a.total_submitted, 0)) * 100 AS \"acceptance_rate\"\n    FROM acceptance_stats a\n    WHERE (a.problem_id, a.collected_at) IN (\n        SELECT problem_id, MAX(collected_at)\n        FROM acceptance_stats\n        GROUP BY problem_id, date_trunc('day', collected_at)\n    )\n),\nranked_stats AS (\n    -- 第二步：針對去重後的資料進行每日排名\n    SELECT \n        s.time,\n        s.acceptance_rate,\n        p.title AS \"problem_title\",\n        ROW_NUMBER() OVER(\n            PARTITION BY s.time \n            ORDER BY s.acceptance_rate ASC\n        ) as rank\n    FROM daily_unique_stats s\n    JOIN problems p ON s.problem_id = p.id\n)\n-- 第三步：只取前 10 名\nSELECT \n    time,\n    acceptance_rate,\n    problem_title\nFROM ranked_stats\nWHERE rank <= 10\nORDER BY time ASC, acceptance_rate ASC;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
